{
  "name": "desktop-note",
  "version": "0.0.1",
  "private": true,
  "main": "main.js",
  "scripts": {
    "help": "webpack -h",
    "clear": "rm -rf dist",
    "lint": "vue-cli-service lint",
    "serve": "vue-cli-service serve",
    "test:e2e": "vue-cli-service test:e2e",
    "start": "electron .",
    "build_dev": "webpack --config ./config/webpack.base.config.js --progress --colors --mode=development",
    "build_dev_watch": "webpack --watch --config ./config/webpack.base.config.js --mode=development --progress --colors",
    "build_pro": "webpack --config ./config/webpack.pro.config.js --mode=production --progress --colors",
    "dev": "run-p server --mode=development",
    "dev_proxy": "run-p server --mode=proxy",
    "server": "webpack-dev-server --host 0.0.0.0 --config ./config/webpack.dev.config.js --progress --colors --profile"
  },
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/polyfill": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/runtime-corejs2": "^7.6.2",
    "babel-loader": "^8.0.0-beta.0",
    "babel-plugin-import": "^1.12.2",
    "core-js": "^2.6.9",
    "lodash": "^4.17.15",
    "moment-locales-webpack-plugin": "^1.1.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.3",
    "vuex": "^3.0.1",
    "webpack-merge": "^4.2.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-e2e-cypress": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^6.0.0",
    "css-loader": "^3.2.0",
    "electron": "^6.0.10",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-vue": "^5.0.0",
    "fast-sass-loader": "^1.5.0",
    "file-loader": "^4.2.0",
    "fs": "^0.0.1-security",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.12.13",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "url-loader": "^2.1.0",
    "vue-loader": "^15.7.1",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9"
  }
}
